{
  "Bybit": {
    "ApiCredentials": {
      "Key": "<value in azure key vault>",
      "Secret": "<value in azure key vault>"
    },
    "UsdPerpetualApiOptions:BaseAddress": "https://api.bybit.com",
    "UsdPerpetualStreamsOptions:BaseAddress": "wss://stream.bybit.com"
  },

  "KeyVaultConfig": {
    "Url": "<value in user secrets>",
    "TenantId": "<value in user secrets>",
    "ClientId": "<value in user secrets>",
    "ClientSecretId": "<value in user secrets>"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "AllowedHosts": "*"
}

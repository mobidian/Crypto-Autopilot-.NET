{
  "KeyVaultConfig": {
    "Url": "<value in azure configuration>",
    "TenantId": "<value in azure configuration>",
    "ClientId": "<value in azure configuration>",
    "ClientSecretId": "<value in azure configuration>"
  },

  "Bybit": {
    "Environment": "Live",

    "ApiCredentials": {
      "Key": "<value in azure key vault>",
      "Secret": "<value in azure key vault>"
    },

    "ApiKeyPermissions": {
      "ReadOnlyKey": "False",
      "Required": [
        "SpotTrade",
        "Order",
        "Position",
        "OptionsTrade",
        "ExchangeHistory"
      ],
      "IpAddressesWhitelist": [
        "*"
      ]
    }
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "AllowedHosts": "*"
}
